	h1.gwr.gTitle Advanced Search
	#quick-search.property-search.gwr
		// se agrega al ul la clase 'hidden-sr' para q se esconda el 'for rent | for sell'
		ul#mini-filters
			li.filter-box
				.gwr
					h4.icon-arrow-select Property search
					.wrap-item
						ul.list-type-sr
							li: button#for-sale.active 
										span For sale
							li: button#for-rent 
										span For Rent
						input(type="search", placeholder="Select Area or Enter City, Zip, Condo Name or Keyword", id="autocomplete-ajax-min")
						.icon-search#submit-ms-min
							input(type="submit", value="Submit")
