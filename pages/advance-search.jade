			li.price
				.gwr
					h4.icon-arrow-select Price Range
					.wrap-item          
						.wrap-inputs
							input(id="price_from", type='text', name='', value="$0")
							span to
							input(id="price_to", type='text', name='', value="$50,000,000")
						.wrap-range
							#range-price.range-slide
						span.direct-results 
							label 4,109 
							|Properties
			li.baths
				.gwr
					h4.icon-arrow-select Bathrooms
					.wrap-item
						.wrap-range
							#range-baths.range-slide
						ul.markers-range
							each bath, a in ['Any', '1', '1.5', '2', '2.5', '3', '3.5', '4', '4.5', '5', '+']
								li #{bath}
						span.direct-results 
							label 4,109 
							|Properties
			li.beds
				.gwr
					h4.icon-arrow-select Bedrooms
					.wrap-item
						.wrap-range
							#range-beds.range-slide
						ul.markers-range
							each beds, e in ['Any', 'Studio', '1', '2', '3', '4', '5', '+']
								li #{beds}
						span.direct-results 
							label 4,109 
							|Properties  
			li.type
				.gwr
					h4.icon-arrow-select Property type
					.wrap-item
						.wrap-checks
							ul
								li
									input(type='checkbox', value='', id="first")
									label(for="first") Single Family Home
								li
									input(type='checkbox', value='', id="second")
									label(for="second") Condo / Townhome
								li
									input(type='checkbox', value='', id="third")
									label(for="third") Vacant Land
			li.living
				.gwr
					h4.icon-arrow-select Living size
					.wrap-item
						.wrap-inputs
							input(id="living_from", type='text', name='', value="0 SF")
							span to
							input(id="living_to", type='text', name='', value="9,000 SF")
						.wrap-range
							#range-living.range-slide
			li.year
				.gwr
					h4.icon-arrow-select Year Built
					.wrap-item
						.wrap-inputs
							input( id="year_from", type='text', name='', value="1961")
							span to
							input( id="year_to", type='text', name='', value="2021")
						.wrap-range
							#range-year.range-slide
			li.waterfront
				.gwr
					h4.icon-arrow-select Waterfront description
					.wrap-item
						.wrap-select.icon-arrow-select
							select
								each waterfront , w in ["Any", "Bay Front", "Canal", "Fixed Bridge", "Intracoastal", "Lake front", "Ocean acces", "Ocean Front", "Point lot", "River front"]
									option(value='') #{waterfront}
			li.parking
				.gwr
					h4.icon-arrow-select Parking spaces
					.wrap-item
						.wrap-select.icon-arrow-select
							select
								option(value='0') Any
								- for(p = 1; p < 4; p++)
									option(value='#{p}') #{p}
								option(value='+') 4+
			li.land
				.gwr
					h4.icon-arrow-select Land size
					.wrap-item
						.wrap-inputs
							input(id="land_from", type='text', name='', value="0 SF")
							span to
							input(id="land_to", type='text', name='', value="9,000 SF")
						.wrap-range
							#range-land.range-slide
			li.features
				.gwr
					h4.icon-arrow-select 
						label Features
						label Lifestyle
					.wrap-item
						.wrap-checks
							ul
								li
									input(type='checkbox', value='', id="check1")
